{
	"extends": "astro/tsconfigs/base",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"paths": {
			"@components/*": ["./src/components/*"],
			"@assets/*": ["./src/assets/*"],
			"@layouts/*": ["./src/layouts/*"],
			"@data/*": ["./src/data/*"]
		}
	}
}
